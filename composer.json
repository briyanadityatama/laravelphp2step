{
  "name": "briyanadityatama/laravelphp2step",
  "description": "Laravel 2 Step Authentication Package",
  "keywords": [
    "laravel",
    "2-step verification",
    "verification"
  ],
  "license": "MIT",
  "type": "package",
  "authors": [
    {
      "name": "Briyan Adityatama",
      "email": "briyanwam@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "briyanadityatama\\laravelphp2step\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "briyanadityatama\\laravelphp2step\\LaravelPHP2stepServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
